{
  "api": {
    "baseUrl": "https://swmtadnpui.execute-api.us-east-1.amazonaws.com/prod",
    "timeout": 10000,
    "fallbackUrl": "http://localhost:3000",
    "note": "Connected to working Lambda API Gateway deployment"
  },
  "topology": {
    "canvas": {
      "defaultLayout": "cose",
      "layouts": [
        "cose",
        "breadthfirst",
        "circle",
        "grid"
      ],
      "maxNodes": 1000,
      "animationDuration": 300
    },
    "controls": {
      "defaultDepth": 2,
      "maxDepth": 5,
      "defaultDirection": "children",
      "directions": ["children", "parents", "both"]
    }
  },
  "ui": {
    "theme": "light",
    "animations": true,
    "autoRefresh": false,
    "refreshInterval": 300000
  },
  "devices": {
    "itemsPerPage": 50,
    "maxItemsPerPage": 200,
    "virtualScrolling": true
  }
}